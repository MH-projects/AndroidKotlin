<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_dev"
    app:startDestination="@id/frgDevs">

    <fragment
        android:id="@+id/frgDevs"
        android:name="com.mh.basickotlin.manuel.view.fragment.FrgDevs"
        android:label="FrgDevs"
        tools:layout="@layout/frg_devs">

        <action
            android:id="@+id/toFrgManuelContent"
            app:destination="@id/frgManuelContent" />

        <action
            android:id="@+id/toFrgAngelMain"
            app:destination="@id/frgAngelMain" />

        <action
            android:id="@+id/toFrgPabloContent"
            app:destination="@id/frgPabloContent" />

        <action
            android:id="@+id/toFrgAntonioContent"
            app:destination="@id/frgAntonioContent" />

        <action
            android:id="@+id/toFrgJosueContent"
            app:destination="@id/frgJosueContent" />

        <action
            android:id="@+id/toFrgJuanContent"
            app:destination="@id/frgJuanContent" />
    </fragment>

    <!-- Content -->
    <fragment
        android:id="@+id/frgManuelContent"
        android:name="com.mh.basickotlin.manuel.view.fragment.FrgManuelContent"
        android:label="frg_manuel_content"
        tools:layout="@layout/frg_manuel_content">
        <action
            android:id="@+id/toFrgManuelCalculator"
            app:destination="@id/frgManuelCalculator" />
    </fragment>

    <fragment
        android:id="@+id/frgAngelMain"
        android:name="com.mh.basickotlin.ui.angel.frgAngelMain"
        android:label="frg_angel_main"
        tools:layout="@layout/frg_angel_main">
        <action
            android:id="@+id/toFrgAngelCalculator"
            app:destination="@id/frgAngelCalculator" />
    </fragment>

    <fragment
        android:id="@+id/frgPabloContent"
        android:name="com.mh.basickotlin.ui.pablo.fragment.FrgPabloContent"
        android:label="frg_pablo_content"
        tools:layout="@layout/frg_pablo_content">
        <action
            android:id="@+id/toFrgPabloCalculator"
            app:destination="@id/frgPabloCalculator" />
    </fragment>

    <fragment
        android:id="@+id/frgAntonioContent"
        android:name="com.mh.basickotlin.antonio.view.view.fragment.FrgAntonioContent"
        android:label="frg_antonio_content"
        tools:layout="@layout/frg_antonio_content">
        <action
            android:id="@+id/toFrgAntonioCalculadora"
            app:destination="@id/frgAntonioCalculator" />
        <action
            android:id="@+id/toFrgAntonioPresenter"
            app:destination="@id/frgAntonioPresenter" />
    </fragment>

    <fragment
        android:id="@+id/frgJosueContent"
        android:name="com.mh.basickotlin.ui.josue.FrgJosueContent"
        android:label="frg_josue_content"
        tools:layout="@layout/frg_josue_content">
        <action
            android:id="@+id/tofrgJosueCalculator"
            app:destination="@id/frgJosueCalculator" />
    </fragment>

    <!-- Calculators -->

    <fragment
        android:id="@+id/frgManuelCalculator"
        android:name="com.mh.basickotlin.manuel.view.fragment.FrgManuelCalculator"
        android:label="FrgManuelCalculator"
        tools:layout="@layout/frg_manuel_calculator" />

    <fragment
        android:id="@+id/frgAngelCalculator"
        android:name="com.mh.basickotlin.ui.angel.FrgAngelCalculator"
        android:label="frgAngelCalculator" />

    <fragment
        android:id="@+id/frgPabloCalculator"
        android:name="com.mh.basickotlin.ui.pablo.fragment.FrgPabloCalculator"
        android:label="FrgPabloCalculator" />

    <fragment
        android:id="@+id/frgAntonioCalculator"
        android:name="com.mh.basickotlin.antonio.view.view.fragment.FrgAntonioCalculator"
        android:label="frg_antonio_calculator"
        tools:layout="@layout/frg_antonio_calculator" />

    <fragment
        android:id="@+id/frgJosueCalculator"
        android:name="com.mh.basickotlin.ui.josue.FrgJosueCalculator"
        android:label="frg_josue_calculator"
        tools:layout="@layout/frg_josue_calculator" />
    <fragment
        android:id="@+id/frgJuanContent"
        android:name="com.mh.basickotlin.ui.juan.activity.fragment.FrgJuanContent"
        android:label="frg_juan_content"
        tools:layout="@layout/frg_juan_content">
        <action
            android:id="@+id/toFrgJuanCalculator"
            app:destination="@id/frgJuanCalculator" />
    </fragment>
    <fragment
        android:id="@+id/frgJuanCalculator"
        android:name="com.mh.basickotlin.ui.juan.activity.fragment.FrgJuanCalculator"
        android:label="FrgJuanCalculator" />
    <fragment
        android:id="@+id/frgAntonioPresenter"
        android:name="com.mh.basickotlin.antonio.view.view.fragment.FrgAntonioPresenter"
        android:label="FrgAntonioPresenter" />
</navigation>