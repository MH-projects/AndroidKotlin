        <action
            android:id="@+id/tofrgJosueCalculator"
            app:destination="@id/frgJosueCalculator" />
        <action
            android:id="@+id/action_frgJosueContent_to_frgJosueWelcome"
            app:destination="@id/frgJosueWelcome" />
    </fragment>

