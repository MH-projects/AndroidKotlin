    <fragment
        android:id="@+id/frgAngelCalculator"
        android:name="com.mh.basickotlin.ui.angel.view.FrgAngelCalculator"
        android:label="FrgAngelCalculator" />
    <fragment
        android:id="@+id/frgAngelInformation"
        android:name="com.mh.basickotlin.ui.angel.frgAngelInformation"
        android:label="frg_angel_information"
        tools:layout="@layout/frg_angel_information" />

    <!--First Proyect-->
    <fragment
        android:id="@+id/frgJosueWelcome"
        android:name="com.mh.basickotlin.josue.view.fragment.FrgJosueWelcome"
        android:label="frg_josue_welcome"
        tools:layout="@layout/frg_josue_welcome" />

    <fragment
        android:id="@+id/frgJuanCalculatorMVP"
        android:name="com.mh.basickotlin.juan.view.fragment.FrgJuanCalculatorMVP"
        android:label="FrgJuanCalculatorMVP" />
    <fragment
        android:id="@+id/FrgJuanContent"
        android:name="com.mh.basickotlin.juan.view.fragment.FrgJuanContent"
        android:label="FrgJuanContent" >
        <action
            android:id="@+id/tofrgJuanCalculatorMVP"
            app:destination="@id/frgJuanCalculatorMVP" />
        <action
            android:id="@+id/toFrgJuanInformation"
            app:destination="@id/frgJuanInformation" />
    </fragment>
    <fragment
        android:id="@+id/frgJuanInformation"
        android:name="com.mh.basickotlin.juan.view.fragment.FrgJuanInformation"
        android:label="frg_juan_information"
        tools:layout="@layout/frg_juan_information" />

</navigation>