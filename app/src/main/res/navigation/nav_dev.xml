        <action
            android:id="@+id/toFrgManuelContent"
            app:destination="@id/frgManuelContent" />

        <action
            android:id="@+id/toFrgAngelMain"
            app:destination="@id/frgAngelMain" />

        <action
            android:id="@+id/toFrgPabloContent"
            app:destination="@id/frgPabloContent" />

        <action
            android:id="@+id/toFrgAntonioContent"
            app:destination="@id/frgAntonioContent" />

        <action
            android:id="@+id/toFrgJosueContent"
            app:destination="@id/frgJosueContent" />
        <action
            android:id="@+id/toFrgJuanContent"
            app:destination="@id/FrgJuanContent" />

    </fragment>

    <!-- Content -->
    <fragment
        android:id="@+id/frgManuelContent"
        android:name="com.mh.basickotlin.manuel.view.fragment.FrgManuelContent"
        android:label="frg_manuel_content"
        tools:layout="@layout/frg_manuel_content">
        <action
            android:id="@+id/toFrgManuelCalculator"
            app:destination="@id/frgManuelCalculator" />
    </fragment>

    <fragment
        android:id="@+id/frgAngelMain"
        android:name="com.mh.basickotlin.ui.angel.frgAngelMain"
        android:label="frg_angel_main"
        tools:layout="@layout/frg_angel_main">
        <action
            android:id="@+id/action_frgAngelMain_to_frgAngelCalculator"
            app:destination="@id/frgAngelCalculator" />
        <action
            android:id="@+id/action_frgAngelMain_to_frgAngelInformation"
            app:destination="@id/frgAngelInformation" />
    </fragment>

    <fragment
        android:id="@+id/frgPabloContent"
        android:name="com.mh.basickotlin.pablo.view.fragment.FrgPabloContent"
        android:label="frg_pablo_content"
        tools:layout="@layout/frg_pablo_content">
        <action
            android:id="@+id/toFrgPabloCalculator"
            app:destination="@id/frgPabloCalculator" />
        <action
            android:id="@+id/toFrgPabloData"
            app:destination="@id/frgPabloData" />
    </fragment>

    <fragment
        android:id="@+id/frgAntonioContent"
        android:name="com.mh.basickotlin.antonio.view.view.fragment.FrgAntonioContent"
