            app:destination="@id/frgAntonioCalculator" />
        <action
            android:id="@+id/toFrgAntonioPresenter"
            app:destination="@id/frgAntonioPresenter" />
    </fragment>

    <fragment
        android:id="@+id/frgJosueContent"
        android:name="com.mh.basickotlin.josue.view.fragment.FrgJosueContent"
        android:label="frg_josue_content"
